{
    "name": "Cosmical",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "activityBar.background": "#0c0c0c",
        "activityBar.foreground": "#c0c0c0",
        "activityBarBadge.background": "#EFC58A",
        "activityBarBadge.foreground": "#0c0c0c",
        "activityBar.activeBorder": "#444",
        "badge.background": "#555",
        "badge.foreground": "#ddd",
        "breadcrumb.foreground": "#d3d3d3",
        "breadcrumbPicker.background": "#212121",
        "button.background": "#444",
        "button.foreground": "#ddd",
        "button.hoverBackground": "#4d4d4d",
        "dropdown.background": "#0c0c0c",
        "dropdown.border": "#303030",
        "dropdown.foreground": "#d3d3d3",
        "editor.background": "#121212",
        "editor.foreground": "#ddd",
        "editor.inactiveSelectionBackground": "#404040",
        "editor.lineHighlightBackground": "#0c0c0c",
        "editor.selectionBackground": "#303030",
        "editor.selectionHighlightBackground": "#303030",
        "editorBracketHighlight.foreground1": "#CC9DEC",
        "editorBracketHighlight.foreground2": "#AC9EF5",
        "editorBracketHighlight.foreground3": "#70A9E5",
        "editorGroup.border": "#404040",
        "editorGroup.dropBackground": "#40404060",
        "editorIndentGuide.activeBackground": "#707070",
        "editorIndentGuide.background": "#404040",
        "editorLineNumber.activeForeground": "#ddd",
        "editorLineNumber.foreground": "#666666",
        "editorWidget.background": "#212121",
        "editorWidget.border": "#404040",
        "focusBorder": "#555",
        "foreground": "#ddd",
        "input.background": "#0c0c0c",
        "input.border": "#303030",
        "input.placeholderForeground": "#888",
        "list.activeSelectionBackground": "#303030",
        "list.activeSelectionForeground": "#ddd",
        "list.activeSelectionIconForeground": "#ddd",
        "list.dropBackground": "#303030",
        "list.focusForeground": "#ddd",
        "list.highlightForeground": "#ddd",
        "list.hoverBackground": "#212121",
        "list.inactiveSelectionBackground": "#303030",
        "list.inactiveSelectionForeground": "#ddd",
        "menu.background": "#212121",
        "menu.foreground": "#ddd",
        "pickerGroup.foreground": "#EFC58A",
        "ports.iconRunningProcessForeground": "#369432",
        "sash.hoverBorder": "#555",
        "selection.background": "#303030",
        "sideBar.background": "#121212",
        "sideBar.border": "#303030",
        "sideBarSectionHeader.background": "#00000000",
        "sideBarSectionHeader.border": "#303030",
        "sideBarTitle.foreground": "#c0c0c0",
        "statusBar.background": "#0c0c0c",
        "statusBar.foreground": "#c0c0c0",
        "statusBarItem.remoteBackground": "#16825d",
        "statusBarItem.remoteForeground": "#ffffff",
        "tab.activeBackground": "#404040",
        "tab.border": "#121212",
        "tab.inactiveBackground": "#212121",
        "tab.lastPinnedBorder": "#cccccc33",
        "textLink.activeForeground": "#E7BB7E",
        "textLink.foreground": "#EFC58A",
        "titleBar.activeBackground": "#0c0c0c",
        "titleBar.activeForeground": "#888",
        "titleBar.inactiveBackground": "#0c0c0c"
    },
    "tokenColors": [
        /* 
        Variables
        ------- 
        */
        {
            "scope": [
                "source",
                "variable.other.object",
                "variable.other.readwrite",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#99DCFF",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#B3E3FF"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "foreground": "#CC9DEC"
            }
        },
        /* 
        Functions, Methods, Properties
        ------- 
        */
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "meta.function"
            ],
            "settings": {
                "foreground": "#A3D789",
                "fontStyle": "italic"
            }
        },
        /* 
        General
        ------- 
        */
        {
            "scope": "text",
            "settings": {
                "foreground": "#ddd"
            }
        },
        {
            "scope": ["support.type.property-name", "meta.object-literal.key"],
            "settings": {
                "foreground": "#99DCFF",
                "fontStyle": ""
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#EFC58A",
                "fontStyle": ""
            }
        },
        /* 
        Keywords, Storage, Constant
        ------- 
        */
        {
            "scope": ["constant.language.boolean", "constant.language"],
            "settings": {
                "foreground": "#CC9DEC"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#E8D67D"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#EDAB8C"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.type.function.arrow",
                "variable.other.global.php"
            ],
            "settings": {
                "foreground": "#70A9E5"
            }
        },
        {
            "scope": [
                "keyword.control",
                "storage.modifier.async",
                "storage.type.function",
                "storage.modifier",
                "keyword.operator.expression",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#AC9EF5",
                "fontStyle": "italic"
            }
        },

        /* 
        Punctuation and Symbols
        ------- 
        */
        {
            "scope": [
                "punctuation.definition.parameters",
                "punctuation.separator.key-value",
                "keyword.operator",
                "keyword.operator.combinator"
            ],
            "settings": {
                "foreground": "#d3d3d3"
            }
        },
        {
            "scope": [
                "punctuation.definition",
                "meta.brace",
                "punctuation.section"
            ],
            "settings": {
                "foreground": "#c0c0c0",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.definition.tag",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#888"
            }
        },
        /* 
        Comments
        -------
        */
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "fontStyle": "italic"
            }
        },
        /* 
        CSS
        -------
        */
        {
            "scope": ["variable.argument.css", "variable.css"],
            "settings": {
                "foreground": "#B3E3FF"
            }
        },
        {
            "scope": "keyword.other.important.css",
            "settings": {
                "foreground": "#AC9EF5"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#A3D789",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.custom",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#A3D789"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#85DBB0"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.css",
                "support.type.property-name.css",
                "support.type.property-name.media",
                "meta.property-name.css"
            ],
            "settings": {
                "foreground": "#99DCFF"
            }
        },
        {
            "scope": [
                "support.constant.property-value",
                "support.constant.color",
                "constant.other.color",
                "support.constant.font-name.css"
            ],
            "settings": {
                "foreground": "#EFC58A"
            }
        },
        /* 
        Javascript
        -------
        */
        {
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#F3836D"
            }
        },
        {
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#85DBB0",
                "fontStyle": ""
            }
        },
        /* 
        HTML tags
        -------
        */
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#70A9E5"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#99DCFF"
            }
        },
        /* Components */
        {
            "scope": "support.class.component",
            "settings": {
                "foreground": "#85DBB0"
            }
        },
        /* 
        MARKDOWN
        -------
        */
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "heading",
            "settings": {
                "foreground": "#99DCFF",
                "fontStyle": "bold"
            }
        }
    ]
}
