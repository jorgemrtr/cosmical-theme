{
    "name": "Cosmical",
    "type": "dark",
    "semanticHighlighting": false,
    "colors": {
        "activityBar.background": "#0c0c0c",
        "activityBar.foreground": "#c0c0c0",
        "activityBarBadge.background": "#ECC58D",
        "activityBarBadge.foreground": "#0c0c0c",
        "activityBar.activeBorder": "#444",
        "badge.background": "#555",
        "badge.foreground": "#ddd",
        "breadcrumb.foreground": "#d3d3d3",
        "breadcrumbPicker.background": "#212121",
        "button.background": "#444",
        "button.foreground": "#ddd",
        "button.hoverBackground": "#4d4d4d",
        "dropdown.background": "#0c0c0c",
        "dropdown.border": "#303030",
        "dropdown.foreground": "#d3d3d3",
        "editor.background": "#121212",
        "editor.foreground": "#d3d3d3",
        "editor.inactiveSelectionBackground": "#404040",
        "editor.lineHighlightBackground": "#0c0c0c",
        "editor.selectionBackground": "#303030",
        "editor.selectionHighlightBackground": "#303030",
        "editorBracketHighlight.foreground1": "#CE88F2",
        "editorBracketHighlight.foreground2": "#A495EF",
        "editorBracketHighlight.foreground3": "#72A6EE",
        "editorGroup.border": "#404040",
        "editorGroup.dropBackground": "#40404060",
        "editorIndentGuide.activeBackground": "#707070",
        "editorIndentGuide.background": "#404040",
        "editorLineNumber.activeForeground": "#d3d3d3",
        "editorLineNumber.foreground": "#666666",
        "editorWidget.background": "#212121",
        "editorWidget.border": "#404040",
        "focusBorder": "#555",
        "foreground": "#d3d3d3",
        "input.background": "#0c0c0c",
        "input.border": "#303030",
        "input.placeholderForeground": "#888",
        "list.activeSelectionBackground": "#303030",
        "list.activeSelectionForeground": "#ddd",
        "list.activeSelectionIconForeground": "#ddd",
        "list.dropBackground": "#303030",
        "list.focusForeground": "#ddd",
        "list.highlightForeground": "#ddd",
        "list.hoverBackground": "#212121",
        "list.inactiveSelectionBackground": "#303030",
        "list.inactiveSelectionForeground": "#ddd",
        "menu.background": "#212121",
        "menu.foreground": "#ddd",
        "pickerGroup.foreground": "#ECC58D",
        "ports.iconRunningProcessForeground": "#369432",
        "sash.hoverBorder": "#555",
        "selection.background": "#303030",
        "sideBar.background": "#121212",
        "sideBar.border": "#303030",
        "sideBarSectionHeader.background": "#00000000",
        "sideBarSectionHeader.border": "#303030",
        "sideBarTitle.foreground": "#c0c0c0",
        "statusBar.background": "#0c0c0c",
        "statusBar.foreground": "#c0c0c0",
        "statusBarItem.remoteBackground": "#16825d",
        "statusBarItem.remoteForeground": "#ffffff",
        "tab.activeBackground": "#404040",
        "tab.border": "#121212",
        "tab.inactiveBackground": "#212121",
        "tab.lastPinnedBorder": "#cccccc33",
        "textLink.activeForeground": "#E7BB7E",
        "textLink.foreground": "#ECC58D",
        "titleBar.activeBackground": "#0c0c0c",
        "titleBar.activeForeground": "#888",
        "titleBar.inactiveBackground": "#0c0c0c"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.object",
                "support.variable.property",
                "support.constant",
                "source.python",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#A7D8F1"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#d3d3d3"
            }
        },
        {
            "scope": ["variable.other.constant"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["variable.other.property"],
            "settings": {
                "foreground": "#A7D8F1"
            }
        },
        {
            "scope": ["variable.other.object.property"],
            "settings": {
                "foreground": "#CE88F2",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#ECC58D"
            }
        },
        {
            "scope": ["variable.parameter"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#F2A988"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#A7D8F1",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["support.function"],
            "settings": {
                "foreground": "#B7E477",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#72A6EE"
            }
        },
        {
            "scope": ["constant.language.boolean", "constant.language"],
            "settings": {
                "foreground": "#F2A988"
            }
        },
        {
            "scope": [
                "keyword.control",
                "storage.modifier.async",
                "storage.type.function",
                "storage.modifier",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#A495EF",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["constant", "constant.numeric"],
            "settings": {
                "foreground": "#F7E897"
            }
        },
        /* 
        Punctuation, Comments and Symbols
        ------- 
        */
        {
            "scope": "punctuation.definition",
            "settings": {
                "foreground": "#c0c0c0"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#888"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#999"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "storage.type.function.arrow",
                "meta.brace",
                "punctuation.definition.parameters"
            ],
            "settings": {
                "foreground": "#c0c0c0"
            }
        },
        /* 
        CSS
        -------
        */
        {
            "scope": ["constant.numeric.css", "constant.other.color"],
            "settings": {
                "foreground": "#F7E897"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#ECC58D"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.custom",
                "support.constant.font-name.css",
                "string.quoted.single.css",
                "string.quoted.double.css",
                "support.constant.property-value",
                "support.constant.color",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#F2A988"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#ECC58D",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.css",
                "support.type.property-name.css",
                "support.type.property-name.media",
                "meta.property-name.css"
            ],
            "settings": {
                "foreground": "#A7D8F1"
            }
        },
        {
            "scope": ["variable.argument.css", "variable.css"],
            "settings": {
                "foreground": "#85E0CE"
            }
        },
        {
            "scope": "keyword.other.important.css",
            "settings": {
                "foreground": "#A495EF"
            }
        },
        /* 
        Javascript, React
        -------
        */
        {
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#F3836D"
            }
        },
        {
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#85E0CE",
                "fontStyle": "italic"
            }
        },
        /* React */
        {
            "scope": "support.class.component",
            "settings": {
                "foreground": "#F3836D"
            }
        },
        /* 
        HTML 
        -------
        */
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#72A6EE"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.js"
            ],
            "settings": {
                "foreground": "#A7D8F1"
            }
        },
        /* 
        MARKDOWN
        -------
        */
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "heading",
            "settings": {
                "foreground": "#A7D8F1",
                "fontStyle": "bold"
            }
        }
    ]
}
