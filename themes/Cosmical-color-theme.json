{
    "name": "Cosmical",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        /* 
        Editor 
        ----------
        */
        "editor.background": "#121212",
        "editor.foreground": "#ddd",
        /* Selection */
        "selection.background": "#3a3a3a",
        "editor.selectionBackground": "#323232",
        "editor.inactiveSelectionBackground": "#2a2a2a",
        /* Line number highlight */
        "editor.lineHighlightBackground": "#2a2a2a",
        "editorLineNumber.activeForeground": "#ddd",
        "editorLineNumber.foreground": "#666",
        /* Breadcrumb */
        "breadcrumb.foreground": "#808080",
        "breadcrumbPicker.background": "#222222",
        /* Bracket highlight colors */
        "editorBracketHighlight.foreground1": "#CC9DEC",
        "editorBracketHighlight.foreground2": "#A899F4",
        "editorBracketHighlight.foreground3": "#65A4E7",
        /* Groups & Tabs */
        "editorGroup.border": "#555",
        "sash.hoverBorder": "#808080",
        "editorGroup.dropBackground": "#42424260",
        "editorIndentGuide.activeBackground": "#666",
        "editorIndentGuide.background": "#424242",
        "editorGroupHeader.tabsBackground": "#222222",
        "editorGroupHeader.noTabsBackground": "#222222",
        "tab.activeBackground": "#424242",
        "tab.inactiveBackground": "#2a2a2a",
        "tab.border": "#121212",
        "tab.lastPinnedBorder": "#cccccc33",

        /* 
        Workspace
        ----------
        */
        "focusBorder": "#666",
        "foreground": "#ddd",
        "textLink.activeForeground": "#DEB67C",
        "textLink.foreground": "#FCCB88",
        /* Title bar (top window bar) */
        "titleBar.activeBackground": "#0a0a0a",
        "titleBar.activeForeground": "#808080",
        "titleBar.inactiveBackground": "#0a0a0a",
        /* Activity bar (left icon bar) */
        "activityBar.background": "#0a0a0a",
        "activityBar.foreground": "#c0c0c0",
        "activityBar.activeBorder": "#424242",
        "activityBarBadge.background": "#FCCB88",
        "activityBarBadge.foreground": "#0a0a0a",
        /* Sidebar (left expandable panel) */
        "sideBar.background": "#121212",
        "sideBar.border": "#424242",
        "sideBarSectionHeader.background": "#00000000",
        "sideBarSectionHeader.border": "#424242",
        "sideBarTitle.foreground": "#ddd",
        /* Status bar (bottom bar) */
        "statusBar.background": "#0a0a0a",
        "statusBar.foreground": "#c0c0c0",
        "statusBarItem.remoteBackground": "#16825d",
        "statusBarItem.remoteForeground": "#ddd",
        /* Badges, Buttons */
        "badge.background": "#424242",
        "badge.foreground": "#ddd",
        "button.background": "#424242",
        "button.hoverBackground": "#3a3a3a",
        "button.foreground": "#f7f7f7",
        /* Dropdown, Input */
        "dropdown.background": "#121212",
        "dropdown.border": "#424242",
        "input.background": "#121212",
        "input.border": "#424242",
        "input.placeholderForeground": "#808080",
        /* Menu */
        "menu.background": "#222222",
        "menu.foreground": "#ddd",

        /* 
        Widgets 
        ----------
        */
        "editorWidget.background": "#222222",
        "editorWidget.foreground": "#ddd",
        "editorWidget.border": "#555",
        /* Find Match */
        "editor.findMatchBackground": "#424242",
        "editor.findMatchBorder": "#85DBB0",
        "editor.findMatchHighlightBackground": "#222222",
        "editor.findMatchHighlightBorder": "#808080",
        "editor.selectionHighlightBackground": "#323232",
        /* Hover */
        "editorHoverWidget.background": "#222222",
        /* Sugest */
        "editorSuggestWidget.background": "#222222",
        "editorSuggestWidget.selectedBackground": "#424242",
        "editorSuggestWidget.selectedForeground": "#f7f7f7",
        "editorSuggestWidget.focusHighlightForeground": "#FCCB88",
        "editorSuggestWidget.highlightForeground": "#f7f7f7",
        /* List */
        "pickerGroup.foreground": "#99DCFF",
        "list.activeSelectionBackground": "#424242",
        "list.activeSelectionForeground": "#f7f7f7",
        "list.activeSelectionIconForeground": "#ddd",
        "list.inactiveSelectionBackground": "#323232",
        "list.inactiveSelectionForeground": "#ddd",
        "list.hoverBackground": "#323232",
        "list.hoverForeground": "#ddd",
        "list.focusBackground": "#323232",
        "list.focusForeground": "#ddd",
        "list.dropBackground": "#222222",

        "terminal.ansiYellow": "#FCCB88",
        "terminal.ansiMagenta": "#CC9DEC",
        "terminal.ansiGreen": "#9BD180",
        "terminal.ansiBlue": "#65A4E7",
        "terminal.ansiCyan": "#99DCFF",
        "terminal.ansiWhite": "#ddd",
        "terminal.ansiRed": "#F3836D",
        "terminal.ansiBlack": "#121212"
    },
    "tokenColors": [
        /* 
        Variables
        ------- 
        */
        {
            "scope": [
                "variable.other.object",
                "variable.other.property",
                "variable.other.readwrite",
                "variable.parameter",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#99DCFF",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#B3E3FF"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "foreground": "#CC9DEC"
            }
        },
        /* 
        Functions, Methods, Properties
        ------- 
        */
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "meta.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#9BD180",
                "fontStyle": "italic"
            }
        },
        /* 
        General
        ------- 
        */
        {
            "scope": "text",
            "settings": {
                "foreground": "#ddd"
            }
        },
        {
            "scope": ["support.type.property-name", "meta.object-literal.key"],
            "settings": {
                "foreground": "#99DCFF",
                "fontStyle": ""
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#FCCB88",
                "fontStyle": ""
            }
        },
        /* 
        Keywords, Storage, Constant
        ------- 
        */
        {
            "scope": ["constant.language.boolean", "constant.language"],
            "settings": {
                "foreground": "#CC9DEC"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#E2D97E"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#EDAB8C"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.type.function.arrow",
                "variable.other.global.php"
            ],
            "settings": {
                "foreground": "#65A4E7",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator.expression",
                "keyword.operator.logical.and",
                "keyword.operator.new",
                "keyword.operator",
                "keyword",
                "storage.modifier",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#A899F4",
                "fontStyle": "italic"
            }
        },

        /* 
        Punctuation and Symbols
        ------- 
        */
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.bitwise",
                "keyword.operator.combinator",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.relational",
                "keyword.operator.spread",
                "keyword.operator.ternary",
                "punctuation.definition.parameters",
                "punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#d3d3d3",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition",
                "meta.brace",
                "punctuation.section"
            ],
            "settings": {
                "foreground": "#c0c0c0",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.definition.tag",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        /* 
        Comments
        -------
        */
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "fontStyle": "italic"
            }
        },
        /* 
        CSS
        -------
        */
        {
            "scope": ["variable.argument.css", "variable.css"],
            "settings": {
                "foreground": "#B3E3FF"
            }
        },
        {
            "scope": "keyword.other.important.css",
            "settings": {
                "foreground": "#A899F4"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#9BD180",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.custom",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#9BD180"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#85DBB0"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.css",
                "support.type.property-name.css",
                "support.type.property-name.media",
                "meta.property-name.css"
            ],
            "settings": {
                "foreground": "#99DCFF"
            }
        },
        {
            "scope": [
                "support.constant.property-value",
                "support.constant.color",
                "constant.other.color",
                "support.constant.font-name.css"
            ],
            "settings": {
                "foreground": "#FCCB88"
            }
        },
        /* 
        Javascript
        -------
        */
        {
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#F3836D"
            }
        },
        {
            "scope": ["entity.name.type.class", "entity.name.type"],
            "settings": {
                "foreground": "#85DBB0",
                "fontStyle": ""
            }
        },
        /* 
        HTML tags
        -------
        */
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#65A4E7"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#99DCFF"
            }
        },
        /* Components */
        {
            "scope": "support.class.component",
            "settings": {
                "foreground": "#85DBB0"
            }
        },
        /* 
        MARKDOWN
        -------
        */
        {
            "scope": [
                "punctuation.definition.list.begin",
                "punctuation.definition.heading",
                "punctuation.definition.bold",
                "punctuation.definition.italic",
                "punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "#65A4E7"
            }
        },
        {
            "scope": "heading",
            "settings": {
                "foreground": "#FCCB88",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#99DCFF"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}
